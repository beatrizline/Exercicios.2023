# Escreva um programa que ler o valor para um lado de um quadrado. Calcule o mostre a área e o perímetro desse quadrado.

def a_lado (l):
    area = l * l
    return area

def p_lado (l):
    perimetro =  l * 4
    return perimetro

def main ():
    l = float(input())
    area = a_lado(l)
    perimetro = p_lado(l)
    print(f'{area:10.4f}')
    print(f'{perimetro:10.4f}')

if __name__ == '__main__':
    main ()
